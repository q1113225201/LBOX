<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--仿支付宝密码输入框-->
    <declare-styleable name="PwdEditText">
        <!--圆角半径-->
        <attr name="borderCornerRadius" format="float" />
        <!--密码长度-->
        <attr name="passwordLength" format="integer" />
        <!--背景颜色-->
        <attr name="bgColor" format="color" />
        <!--线宽度-->
        <attr name="lineWidth" format="float" />
        <!--线颜色-->
        <attr name="lineColor" format="color" />
        <!--密码点半径-->
        <attr name="dotRadius" format="float" />
        <!--点颜色-->
        <attr name="dotColor" format="color" />
    </declare-styleable>
    <!--左滑删除控件-->
    <declare-styleable name="SwipeItemLayout">
        <!--能否滑动-->
        <attr name="isSwipeEnable" format="boolean" />
        <!--是否左滑-->
        <attr name="isLeftSwipe" format="boolean" />
    </declare-styleable>
    <!--圆形进度条-->
    <declare-styleable name="CircleProgress">
        <!--偏移角度-->
        <attr name="progressOffsetAngle" format="float" />
        <!--进度条颜色-->
        <attr name="progressColor" format="color" />
        <!--进度条背景颜色-->
        <attr name="progressBackgroundColor" format="color" />
        <!--进度条宽度-->
        <attr name="progressWidth" format="float" />
        <!--最大进度-->
        <attr name="progressMax" format="float" />
        <!--字体颜色-->
        <attr name="textColor" format="color" />
    </declare-styleable>
    <!--节点进度-->
    <declare-styleable name="NodeProgress">
        <!--偏移角度-->
        <attr name="nodeProgressOffsetAngle" format="float" />
        <!--进度条颜色-->
        <attr name="nodeProgressColor" format="color" />
        <!--进度条背景颜色-->
        <attr name="nodeProgressBackgroundColor" format="color" />
        <!--进度条宽度-->
        <attr name="nodeProgressWidth" format="float" />
        <!--最大进度-->
        <attr name="nodeProgressMax" format="float" />
        <!--进度条类型-->
        <attr name="nodeProgressStyle" format="enum">
            <!--圆弧形-->
            <enum name="arc" value="0" />
            <!--线形-->
            <enum name="line" value="1" />
        </attr>
        <!--节点个数-->
        <attr name="nodeCount" format="integer" />
        <!--节点宽度-->
        <attr name="nodeWidth" format="float" />
    </declare-styleable>
</resources>