<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.sjl.lbox">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission
        android:name="android.permission.PACKAGE_USAGE_STATS"
        tools:ignore="ProtectedPermissions" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:name=".LBoxApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:sharedUserId="android.uid.system"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".app.LoadingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".app.MainActivity" />
        <activity android:name=".app.webview.WebViewActivity" />
        <activity android:name=".app.network.monitor.NetworkMonitorActivity" />
        <activity android:name=".app.image.ImageActivity" />
        <activity android:name=".app.contact.ContactActivity" />
        <activity android:name=".app.QRCode.QRCodeActivity" />
        <activity android:name=".app.QRCode.BGAQRCode.BGAQRCodeActivity" />
        <activity
            android:name=".app.QRCode.BGAQRCode.ScanActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".app.signature.SignatureActivity"
            android:windowSoftInputMode="stateHidden|stateAlwaysHidden" />
        <activity
            android:name=".app.QRCode.MyQRCode.activity.CaptureActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".app.QRCode.MyQRCode.MyQRCodeScanActivity" />
        <activity android:name=".app.progress.ProgressActivity" />
        <activity android:name=".app.http.HttpActivity" />
        <activity android:name=".app.http.NoHttp.NoHttpActivity" />
        <activity android:name=".app.Notification.NotificationActivity" />
        <activity android:name=".app.Notification.NotificationClickActivity" />
        <activity android:name=".app.Notification.NotificationClickPictureActivity" />
        <activity android:name=".app.zip.ZipActivity" />
        <activity android:name=".app.compass.CompassActivity" />
        <activity android:name=".app.pedometer.PedometerActivity" />

        <service
            android:name=".app.pedometer.service.StepService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".app.gesture.GestureActivity" />
        <activity android:name=".app.animate.AnimateActivity" />
        <activity android:name=".app.animate.FrameAnimateActivity" />
        <activity android:name=".app.animate.TweenAnimateActivity" />
        <activity android:name=".app.animate.PropertyAnimateActivity" />
        <activity android:name=".app.viewpager.ViewPagerActivity" />
        <activity android:name=".app.viewpager.AdvertiseActivity" />
        <activity android:name=".app.music.MusicActivity" />
        <activity android:name=".app.FloatWindow.FloatWindowActivity" />

        <service
            android:name=".app.FloatWindow.service.FloatWindowService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".app.EditText.EditTextActivity" />
        <activity android:name=".app.EditText.PwdEditText.PwdEditTextActivity" />
        <activity android:name=".app.mode.ModeActivity" />
        <activity android:name=".app.mode.mvp.MVPActivity" />
        <activity android:name=".app.mode.mvp.login.MVPLoginActivity" />
        <activity android:name=".app.mode.mvp.main.MVPMainActivity"></activity>
    </application>

</manifest>